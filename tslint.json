{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "prettier": true,
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "allow-leading-underscore",
      "allow-trailing-underscore"
    ],
    "no-console": [true, "log"]
  },
  "rulesDirectory": []
}
